# Конфигурация для Telegram бота магазина

# Токен бота (обязательно)
BOT_TOKEN=your_bot_token_here

# ID администраторов через запятую (обязательно)
ADMINS=123456789,987654321

# URL WebApp (обязательно для работы магазина)
WEBAPP_URL=https://your-domain.com/webapp

# Токен провайдера для Telegram Payments (опционально)
PROVIDER_TOKEN=your_provider_token_here

# Уровень логирования (DEBUG, INFO, WARNING, ERROR, CRITICAL)
LOG_LEVEL=INFO

# Дополнительные настройки
REPL_URL=https://your-repl.repl.co
REPL_SLUG=your-repl-slug
